<resources>

    <attr name="cropImageStyle" format="reference" />
    <declare-styleable name="CropImageView">
        <attr name="highlightColor" format="reference|color" />
        <attr name="showThirds" format="boolean" />
        <attr name="showCircle" format="boolean" />
        <attr name="showHandles">
          <enum name="changing" value="0" />
          <enum name="always" value="1" />
          <enum name="never" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ProgressWheel">
        <attr name="matProg_progressIndeterminate" format="boolean" />
        <attr name="matProg_barColor" format="color" />
        <attr name="matProg_rimColor" format="color" />
        <attr name="matProg_rimWidth" format="dimension" />
        <attr name="matProg_spinSpeed" format="float" />
        <attr name="matProg_barSpinCycleTime" format="integer" />
        <attr name="matProg_circleRadius" format="dimension" />
        <attr name="matProg_fillRadius" format="boolean" />
        <attr name="matProg_barWidth" format="dimension" />
        <attr name="matProg_linearProgress" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BannerView">
        <attr name="default_image" format="reference" />
        <attr name="delay_time" format="integer" />
        <attr name="is_auto_play" format="boolean" />
        <attr name="title_background" format="color|reference" />
        <attr name="title_textcolor" format="color" />
        <attr name="title_textsize" format="dimension" />
        <attr name="title_height" format="dimension" />
        <attr name="indicator_width" format="dimension" />
        <attr name="indicator_height" format="dimension" />
        <attr name="indicator_margin" format="dimension" />
        <attr name="indicator_drawable_selected" format="reference" />
        <attr name="indicator_drawable_unselected" format="reference" />
        <!--<attr name="indicator_animator" format="reference" />-->
        <!--<attr name="indicator_animator_reverse" format="reference" />-->
    </declare-styleable>
    <declare-styleable name="GradientTabStrip">
        <attr name="gtsTextSize" format="dimension"/>
        <attr name="gtsTextColor" format="color"/>
        <attr name="gtsDrawablePadding" format="dimension"/>
        <attr name="gtsBackground" format="reference"/>
        <attr name="gtsInterval" format="reference" />
        <attr name="gtsMinItemWidth" format="dimension"/>
        <attr name="gtsTagTextSize" format="dimension" />
        <attr name="gtsTagTextColor" format="color" />
        <attr name="gtsTagBackground" format="reference" />
        <attr name="gtsTagPadding" format="dimension" />
        <attr name="gtsTagPaddingLeft" format="dimension" />
        <attr name="gtsTagPaddingTop" format="dimension" />
        <attr name="gtsTagPaddingRight" format="dimension" />
        <attr name="gtsTagPaddingBottom" format="dimension" />
        <attr name="gtsTagMargin" format="dimension" />
        <attr name="gtsTagMarginLeft" format="dimension" />
        <attr name="gtsTagMarginTop" format="dimension" />
        <attr name="gtsTagMarginRight" format="dimension" />
        <attr name="gtsTagMarginBottom" format="dimension" />
        <attr name="gtsTagMinSizeMode" format="enum">
            <enum name="OnlyHasText" value="0" />
            <enum name="Always" value="1" />
        </attr>
        <attr name="gtsTagMinWidth" format="dimension" />
        <attr name="gtsTagMinHeight" format="dimension" />
        <attr name="gtsCenterGap" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="TitleBar">
        <attr name="titleBarTitle" format="string"/>
        <attr name="titleBarRightText" format="string"/>
        <attr name="titleBarLeftImage" format="reference"/>
        <attr name="titleBarRightImage" format="reference"/>
        <attr name="titleBarBackground" format="reference|color"/>
    </declare-styleable>

    <!--                 AutoLayout  Start               -->
    <declare-styleable name="AutoLayout_Layout">
        <attr name="layout_auto_basewidth">
            <flag name="width" value="1"/>
            <flag name="height" value="2"/>
            <flag name="textSize" value="4"/>
            <flag name="padding" value="8"/>
            <flag name="margin" value="16"/>
            <flag name="marginLeft" value="32"/>
            <flag name="marginTop" value="64"/>
            <flag name="marginRight" value="128"/>
            <flag name="marginBottom" value="256"/>
            <flag name="paddingLeft" value="512"/>
            <flag name="paddingTop" value="1024"/>
            <flag name="paddingRight" value="2048"/>
            <flag name="paddingBottom" value="4096"/>
        </attr>

        <attr name="layout_auto_baseheight">
            <flag name="width" value="1"/>
            <flag name="height" value="2"/>
            <flag name="textSize" value="4"/>
            <flag name="padding" value="8"/>
            <flag name="margin" value="16"/>
            <flag name="marginLeft" value="32"/>
            <flag name="marginTop" value="64"/>
            <flag name="marginRight" value="128"/>
            <flag name="marginBottom" value="256"/>
            <flag name="paddingLeft" value="512"/>
            <flag name="paddingTop" value="1024"/>
            <flag name="paddingRight" value="2048"/>
            <flag name="paddingBottom" value="4096"/>
            <flag name="minWidth" value="8192"/>
            <flag name="maxWidth" value="16384"/>
            <flag name="minHeight" value="32768"/>
            <flag name="maxHeight" value="65536"/>
        </attr>
    </declare-styleable>
    <!--                 AutoLayout  End               -->
</resources>